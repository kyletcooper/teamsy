{
	"name": "wrd/teamsy",
	"description": "A library for building nested, team-based permissions systems for Laravel.",
	"version": "0.0.1",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"WRD\\Teamsy\\": "src/"
		}
	},
	"authors": [
		{
			"name": "kylethomascooper",
			"email": "kyle@wrd.agency"
		}
	],
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"laravel/framework": "^10",
		"wrd/sleepy": "dev-main"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"preferred-install": "dist",
		"optimize-autoloader": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"WRD\\Teamsy\\Providers\\TeamsyServiceProvider"
			],
			"alias": [
				"WRD\\Teamsy\\Support\\Facades\\Roles"
			]
		}
	}
}
